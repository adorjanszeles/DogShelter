<!doctype html>
<html class="no-js" lang="hu">
<%- include head/head %>
<body>

<%- include menu/menu %>

<div class="hero-content">
    <div class="large-8 columns">
        <h2>Felelős felhasználó elérhetősége</h2>
    </div>
</div>

<section class="hero-content">
    <div class="row">
        <div class="large-8 columns large-centered">
            <label for="name">Név</label>
            <input id="name" name="name" type="text" maxlength="20" disabled="yes" value="<%= tpl.user.userName %>"/>
            <label for="email">E-mail cím</label>
            <input id="email" name="email" type="text" maxlength="20" disabled="yes" value="<%= tpl.user.email %>"/>
            <label for="mobile">Telefonszám</label>
            <input id="mobile" name="mobile" type="text" maxlength="20" disabled="yes"
                   value="<%= tpl.user.phone %>"/>
            <label for="address">Lakcím</label>
            <input id="address" name="address" type="text" maxlength="20" disabled="yes"
                   value="<%= tpl.user.address %>"/>
            <a href="javascript:history.back()"><input id="back" name="back" value="Vissza" type="Button"/></a>
        </div>
    </div>
</section>
</body>
</html>
